# Discord Bot Token - Get this from https://discord.com/developers/applications
DISCORD_TOKEN=your_discord_bot_token_here

# Channel IDs where the bot will spawn Pokemon (comma-separated)
# Example: SPAWN_CHANNELS=123456789,987654321
SPAWN_CHANNELS=

# Random spawn interval in seconds (Pokemon will spawn randomly between MIN and MAX)
SPAWN_INTERVAL_MIN=180
SPAWN_INTERVAL_MAX=600
